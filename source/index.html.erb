<script type="text/javascript">
  $(function() {
    $(window).scroll(function (event) {
        var scroll = $(window).scrollTop();
        var heroScroll = $('.hero').scrollTop() + $('.hero').height() - 70;

        if (scroll >= heroScroll) {
          $('nav').removeClass('trans');
        }
        else {
          $('nav').addClass('trans');
        }
    });
  });
</script>